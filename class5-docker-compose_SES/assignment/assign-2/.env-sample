# LOG_DIR= directory containing logs to be rotated
# ARCHIVE_DIR= Directory to store archived logs
# INTERVAL_MINUTES= Interval in minutes between rotations
# RETENTION_DAYS= Number of days to retain archived logs
# MODE= Operation mode: 'once' for single rotation, 'loop' for continuous

LOG_DIR=logs
ARCHIVE_DIR=archive
INTERVAL_MINUTES=60
RETENTION_DAYS=5
MODE=once
